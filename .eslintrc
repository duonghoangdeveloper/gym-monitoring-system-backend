{
  "extends": [
    "wesbos"
  ],
  "plugins": [
    "simple-import-sort",
    "sort-keys-fix",
    "sort-destructure-keys"
  ],
  "rules": {
    "no-nested-ternary": "off",
    "no-use-before-define": "off",
    "import/no-cycle": "off",
    "no-plusplus": "off",
    "sort-imports": "off",
    "import/order": "off",
    "simple-import-sort/sort": "warn",
    "sort-keys-fix/sort-keys-fix": "warn",
    "sort-destructure-keys/sort-destructure-keys": 2
  }
}